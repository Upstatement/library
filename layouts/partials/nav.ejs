<div class="g-upper-body">
  <% if (locals.title) { %>
    <% if (locals.duplicates) { %>
      <%- include('../partials/warning', {
        message: template(
          'warning.duplicate',
          locals.duplicates,
          locals.parentId
        ) }
      ) %>
    <% } %>
  <h1 class="headline"><%= title %></h1>
  <% } %>
</div>

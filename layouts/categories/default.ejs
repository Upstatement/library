<!DOCTYPE html>
<html>
  <%- include ../partials/head %>
  <body>
    <%- include('../partials/header') %>

    <main id="categories-page" class="site-page">
      <div class="page-inner">
        <div class="page-width">
          <div class="content__width">
            <h1 class="headline"><%= title %></h1>
          </div>
          <div class="g-body">
            <div class="g-main-content">
              <div class="content__width">
                <% if (locals.content) { %> <%- content %> <% } %> <% if
                (locals.children && children.length) { %> <%-
                include('../partials/childrenList', { children, kicker:
                template('folder.childrenList.kicker', title) }) %> <% } %>
              </div>
            </div>

            <%- include('../partials/footer', { pageType: 'document',
            topLevelFolder: url.split('/')[1] }) %>
          </div>
        </div>
      </div>
    </main>
  </body>
</html>
